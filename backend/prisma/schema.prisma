datasource db {
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model MedicineItem {
  id            Int     @id @default(autoincrement())
  userName      String
  age           Int
  contact       String
  profileImage  String? 
  documentProof String? 
  drugName      String
  medicineType  String
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

